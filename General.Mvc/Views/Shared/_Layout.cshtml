<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] </title>
    <link href="/assets/css/bootstrap.min.css" rel="stylesheet" />
    <link href="/assets/css/font-awesome.min.css" rel="stylesheet" />
    <link href="/assets/css/jquery.gritter.css" rel="stylesheet" />
    <link href="/assets/css/ace-fonts.css" rel="stylesheet" />
    <link href="/assets/css/ace.min.css" rel="stylesheet" />
    <link href="/assets/css/ace-skins.min.css" rel="stylesheet" />
    <link href="/assets/css/ace-rtl.min.css" rel="stylesheet" />
    <link href="/assets/css/ace-part2.min.css" rel="stylesheet" />
    <link href="/assets/css/jquery-ui.custom.min.css" rel="stylesheet" />
    <link href="/assets/css/bootstrap-editable.css" rel="stylesheet" />
    <link href="/assets/css/datepicker.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" />
    <script src="/assets/js/ace-extra.min.js"></script>
    <script src="~/assets/js/jquery.min.js"></script>
    
</head>
<body class="no-skin">
    <div id="navbar" class="navbar navbar-default navbar-fixed-top">
        <div class="navbar-container" id="navbar-container">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <div class="navbar-header pull-left">
                <a asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand">图书馆座位系统</a>
            </div>
            <div class="navbar-buttons navbar-header">
                <ul class="nav navbar-nav">
                    <li>
                        <a href="@Url.RouteUrl("firstFloor")">负一层</a>
                    </li>
                    <li>
                        <a href="Second.html">第一层</a>
                    </li>
                    <li>
                        <a href="#">第二层</a>
                    </li>

                    <li>
                        <a href="#">第三层</a>
                    </li>
                    <li>
                        <a href="#">第四层</a>
                    </li>
                </ul>
            </div>
            @{ Html.RenderPartial("_PublicLoginPartial");}
        </div>
    </div>
    <div class="main-container">
        <div class="main-content">
            @RenderBody()
        </div>
        <hr />
    </div>
    <div class="row"> 
        <div class="col-sm-12 center">
            <footer>
                <p>&copy; 2019 图书馆座位系统</p>
            </footer>
        </div>
        
    </div>
    <script src="~/assets/js/bootstrap.min.js"></script>
    <script src="~/assets/js/ace.min.js"></script>
    <script src="~/assets/js/ace-elements.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
    <script src="~/assets/js/bootbox.min.js"></script>
    <script src="~/js/jquery.modal.js"></script>
    <script src="~/js/jquery.form.js"></script>
    <script src="~/js/jquery.ajax.setup.js"></script>
    <script>

        $(document).ready(function(e) {
             var url = '@Url.RouteUrl("UpdateOrder")';
            $.get(url);
        });


    </script>
    @RenderSection("scripts", required: false)

    @RenderSection("Scripts", required: false)

    
</body>
</html>
